@model IEnumerable<MVC_CC_1.Models.CodeFirst.Movie>

<h2>The movie list is</h2>

<table>
    <tr>
        <th>Title</th>
        <th>Year</th>
    </tr>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Moviename</td>
            <td>@movie.DateofRelease</td>
        </tr>
    }
</table>
